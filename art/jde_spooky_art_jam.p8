function _init()
	update_every=2
	update_count=0
	
	frame=0
	max_frames=32
	
	sswidth=8
	
	sx=0
	sy=0
	
	s_width=16
	s_height=16
end

function _update60()
	
	update_count+=1
	
	if update_count>=update_every then
		
		update_count=0
		frame+=1
		sx+=s_width
		
		if frame>=max_frames then
			frame=0
			sx=0
			sy=0
		end
		
		if sx>=sswidth*s_width then
			sx=0
			sy+=s_height
		end
		
	end
	
end

function _draw()
	cls()
	sspr(sx,sy,16,16,0,0,32,32)
end
