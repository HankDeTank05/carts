--viz: math

function _init()
	options={
	"matrix * vector",
	"vector * matrix",
	"matrix * matrix"}
	
	cur=1
end

function _update60()

	if btnp(⬆️) then
		cur-=1
		if cur<=0 then
			cur=#options
		end
	elseif btnp(⬇️) then
		cur+=1
		if cur>#options then
			cur=1
		end
	end

end

function _draw()
	
	cls()
	
	print(">",0,6*cur,11)
	
	for i=1,#options do
		print(options[i],5,6*i,11)
	end
	
end
